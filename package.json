{
  "name": "pixi_boilerplate",
  "version": "1.0.0",
  "description": "simple boilerplate for 2d games using pixi.js, typescript and webpack",
  "main": "src/js/main.js",
  "keywords": [
    "pixi.js",
    "webpack"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode development",
